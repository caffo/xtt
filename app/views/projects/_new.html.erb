<% form_for current_user.owned_projects.new do |f| %>
  <p class='required'>
    <label>Project Name</label>
    <%= f.text_field :name %>
  </p>
  <p class='required'>
    <label>Code</label> (used in messages)
    <%= f.text_field :code, :size => 5 %>
  </p>

  <p class='btns'>
    <%= f.submit "Create" %>
    or <%= link_to_function "cancel", "InlineForm.toggleAdd(this)" %>
  </p>
<% end %>
